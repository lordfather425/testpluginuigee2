{
  "manifest_version": 2,
  "name": "Car Listing Price",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Car Listing Price"
  }
}
